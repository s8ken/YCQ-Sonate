    "@mui/material-pigment-css": "latest",
    "@types/react": "latest",
    "ajv": "latest",
    "ajv-formats": "latest",
    "asn1.js": "latest",
    "axios": "latest",
    "bcryptjs": "latest",
    "canvas": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "latest",
    "cors": "latest",
    "crypto": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-async-handler": "latest",
    "express-mongo-sanitize": "latest",
    "express-rate-limit": "latest",
